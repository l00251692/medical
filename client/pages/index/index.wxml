<view class="background">
  <image src="../../images/index/background.png" mode='widthFit'></image>
</view>

<view class='title'>
  <text>Welcome to</text>
  <text>小程序名字替换掉</text>
</view>

<view class="container">
  <view class="logo">
    <image src='../../images/index/logo.png' mode='aspectFit'></image>
    <navigator url="/pages/agreement/agreement" class='navigator'>
      <text class="info_link">《医疗服务协议》</text>
    </navigator>
  </view>

  <checkbox-group class="checkbox" bindchange="toAgree">
    <label>
      <checkbox checked="{{agree}}" />
      <text class="xieyi">我已阅读并接受上述协议</text>
    </label>
  </checkbox-group>

  <view class="content">
    <button class="create-btn" bindtap="createOrder">创建订单</button>
  </view>
</view>